{
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "moduleResolution": "Node",
        "declaration": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": false,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "esModuleInterop": true,
        "skipLibCheck": false,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "sourceMap": true,
        "types": [
            "node"
        ],
        "incremental": true,
        "tsBuildInfoFile": "tsconfig.tsbuildinfo"
    },
    "files": [
        "index.ts",
        "src/NginxBlockManager.ts",
        "src/NginxBlockTypes.ts"
    ]
}
